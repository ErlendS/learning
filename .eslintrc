{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
      "es6": true,
      "node": true
  },
  "rules": {
    "eqeqeq": [2, "allow-null"],
    "no-var": 2,
    "prefer-const": 1,
    "object-shorthand": 1,
    "quote-props": [2, "as-needed"],
    "no-undef": 2,
    "no-unused-vars": 1,
  }
}
